<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- #if VERSION >= "17.0" -->
        <record id="discuss_channel_view_form" model="ir.ui.view">
            <field name="name">discuss.channel.form.ai_agent</field>
            <field name="model">discuss.channel</field>
            <field name="inherit_id" ref="mail.discuss_channel_view_form" />
            <field name="arch" type="xml">
                <field name="description" position="after">
                    <field name="ai_quest_id" />
                </field>
            </field>
        </record>
        <!-- #elif VERSION <= "16.0" -->
        <record id="mail_channel_view_form" model="ir.ui.view">
            <field name="name">mail.channel.form.ai_agent</field>
            <field name="model">mail.channel</field>
            <field name="inherit_id" ref="mail.mail_channel_view_form" />
            <field name="arch" type="xml">
                <field name="description" position="after">
                    <field name="ai_quest_id" />
                </field>
            </field>
        </record>
        <!-- #endif -->
    </data>
</odoo>
