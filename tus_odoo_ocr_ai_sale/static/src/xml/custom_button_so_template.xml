<?xml version="1.0" encoding="utf-8"?>
<templates>
    <t t-inherit="account.FileuploadListView.Buttons" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <t t-if="props.resModel == 'sale.order'">
                 <button type="button" class="btn btn-primary" style="margin-left: 10px;" t-on-click="onClickOCRSale">
                    Create Via OCR
                </button>
            </t>
        </xpath>
    </t>
</templates>
