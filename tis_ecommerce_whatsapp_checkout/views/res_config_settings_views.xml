<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.whatsapp</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='website_sale']" position="inside">
                <group string="WhatsApp Checkout">
                    <field name="whatsapp_number"/>
                    <field name="whatsapp_checkout"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>