<odoo>
    <record id="website_form_inherit_whatsapp" model="ir.ui.view">
        <field name="name">website.form.inherit.whatsapp</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="after">
                <group string="WhatsApp Settings">
                    <field name="whatsapp_number"/>
                    <field name="whatsapp_check_out"/>
                    <field name="whatsapp_checkout"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>